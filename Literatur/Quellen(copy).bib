@book{Gregoire2021,
	author={Gregoire, Marc},
	publisher = {John Wiley \& Sons, Ltd},
	isbn = {9781119695547},
	title = {Professional C++},
	edition={5},
	doi = {https://doi.org/10.1002/9781119695547.ch6},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781119695547.ch6},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/9781119695547.ch6},
	year = {2021},
	keywords = {abstraction, reusable code, reuse philosophy, SOLID principles, uncluttered interfaces},
	abstract = {Summary This chapter helps the reader learn how they should design reusable code. The most important strategy for designing reusable code is abstraction. The chapter presents specific tips for structuring the readers' code: to avoid combining unrelated or logically separate concepts, to use templates for generic data structures and algorithms, to provide appropriate checks and safeguards, and to design for extensibility. It also presents six strategies for designing interfaces: to follow familiar ways of doing things, to not omit required functionality, to present uncluttered interfaces, to provide documentation, to provide multiple ways to perform the same functionality, and to provide customizability. The chapter discusses how to reconcile the often-conflicting demands of generality and ease of use. It concludes with SOLID, an easy-to-remember acronym that describes the most important design principles.}
}