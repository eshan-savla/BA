	author = {Rachmadi, Reza Fuad and Uchimura, Keiichi and Koutaki, Gou and Ogata, Kohichi},
	date = {2017-09},
	keywords = {Autonomous automobiles, Convolutional codes, Image edge detection, Laser radar, Roads, Three-dimensional displays, Training},
	file = {IEEE Xplore Abstract Record:/home/eshan/Zotero/storage/KDXD4HX9/8228570.html:text/html;IEEE Xplore Full Text PDF:/home/eshan/Zotero/storage/C5ZSJNU5/Rachmadi et al. - 2017 - Road edge detection on 3D point cloud data using E.pdf:application/pdf},
